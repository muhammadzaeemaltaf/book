{
  "version": 2,
  "builds": [
    {
      "src": "src/api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/api/main.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.11"
    }
  }
}
